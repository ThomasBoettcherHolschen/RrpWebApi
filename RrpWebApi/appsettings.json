{
        "Serilog": {
                "Using": [  "Serilog.Sinks.Console"],
                "MinimumLevel": {
                        "Default": "Information",
                        "Override": {
                                "Microsoft": "Warning",
                                "System": "Warning",
                                "Quartz": "Warning"
                        }
                },
                "WriteTo": [
                        {
                                "Name": "Console",
                                "Args": {
                                        "outputTemplate": "{Timestamp:o} [{Level:u3}] {Message} <s:{SourceContext}> {NewLine}{Exception}"
                                }
                        }
                ],
                "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
                "Properties": {
                        "ProcessName": "RRP-Demo"
                }
        }
}